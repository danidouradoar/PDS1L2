#include <stdio.h>

int main(){
    int x;
    int i =1;
    int m2 = 0;
    int m3 = 0;
    int m5 = 0;
    int mt = 0;

    scanf("%d\n", &x);
    
    while (x>=i){
        (i%2 == 0)? m2++: m2;
        (i%3 == 0)? m3++: m3;
        (i%5 == 0)? m5++: m5;
        (i%2 == 0 && i%3 == 0 && i%5 == 0)? mt++: mt;
        i++;
    }
    printf("Múltiplos de 2: %d\n", m2);
    printf("Múltiplos de 3: %d\n", m3);
    printf("Múltiplos de 5: %d\n", m5);
    printf("Múltiplos de todos: %d\n", mt);

    return 0;
}
